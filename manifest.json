{
    "manifest_version": 3,
    "name": "FastGPT",
    "description": "Adds up/down arrows (and user key arrow inputs) to travel up to the last prompt or down to the next prompt on Chat GPT",
    "version": "2.0",
    "icons": {
        "16": "images/icon-16.jpg",
        "32": "images/icon-32.jpg",
        "48": "images/icon-48.jpg",
        "128": "images/icon-128.jpg"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://chatgpt.com/c/*"
            ]
        }
    ]
}
